{
	"defaultSeverity": "error",
	"extends": [
		"tslint-config-airbnb",
		"tslint-config-prettier"
	],
	"jsRules": {},
	"rules": {
		"variable-name": false,
		"function-name": false,
		"no-increment-decrement": false,
		"no-parameter-reassignment": false,
		"no-else-after-return": false,
		"triple-equals": true,
		"indent": [true, "tabs", 4],
		"semicolon": [
			true,
			"always"
		],
	},
	"rulesDirectory": []
}